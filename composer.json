{
    "name": "pawel/laravel-articles",
    "description": "A Laravel package for managing articles.",
    "type": "library",
    "version": "1.0.0",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Pawel\\Articles\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pawel\\Articles\\ArticleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
